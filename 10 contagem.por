
programa
{
    funcao inicio()
    {
        // Declaração da variável
        inteiro i
        
        // Exibe a contagem regressiva de 10 a 1
        para (i = 10; i >= 1; i--)
        {
            escreva(i, "\n")
        }
        
        // Mensagem após a contagem
        escreva("Contagem regressiva finalizada!")
    }
}